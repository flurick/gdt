script/source = "extends VSlider

export (int) var bus_id = 0 


func _ready():
	connect(\"value_changed\",self,\"ctrl_bus\")

var selected_effect
var selected_bus_id = 0
func ctrl_bus(value):
	
	selected_bus_id = get_node(\"../bus\").get_item_metadata( get_node(\"../bus\").selected )
	selected_effect = AudioServer.get_bus_effect(selected_bus_id, 0)
	if selected_effect:
		print(selected_effect, selected_effect.get_class())
		match selected_effect.get_class():
			\"AudioEffectReverb\":  selected_effect.room_size = 1*value
			\"AudioEffectChorus\":  selected_effect.voice_count = int(4*value)
			\"AudioEffectFilter\": selected_effect.cutoff_hz = 40000*value
			
