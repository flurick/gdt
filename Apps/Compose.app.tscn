script/source = "extends Control

var mark = 0
var last_mark = 0


func _process(delta):
	
	if $HBoxContainer.get_child(last_mark): 	$HBoxContainer.get_child(last_mark).find_node(\"dot\").modulate = ColorN(\"white\")
	if $HBoxContainer.get_child(mark):  $HBoxContainer.get_child(mark).find_node(\"dot\").modulate = ColorN(\"red\")
	
	last_mark = mark
	mark += 1
	if mark >= $HBoxContainer.get_child_count(): 
		mark = 0"

